{
  "compilerOptions": {
    "target": "es5",
    "typeRoots": [
      "./src/typing-stubs",
      "./node_modules/@types"
    ],
    "module": "commonjs",
    "lib": [
      "es6"
    ],
    "allowJs": true,
    "outDir": "build",
    "rootDir": "src",
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true
  }
}